<h1>Tasks#index</h1>


<% @tasks.each do |t| %>
    <%= link_to t.name, task_path(t) %>
   <% if t.open %>
   <p>a</p>
   <% else %>
   <p>b</p>
   <%end%>
<% end %>

<% if session[:role] == 'admin' %>
    <%= link_to "Members", members_path %>
    <%= link_to "Users", users_path %>
    <%= link_to "Groups", groups_path %>
<%end%>
<%= link_to "Cancel", tasks_path %>
